{
  "name": "example-node-ts",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node src/main.ts",
    "debug": "NODE_ENV=development DEBUG='@neo4j/graphql:execute,Server:*,Neo4j:*' ts-node-dev --exit-child --respawn src/main.ts",
    "build": "node ./build.js",
    "check": "tsc --pretty --noEmit",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@neo4j/graphql": "^3.16.1",
    "@neo4j/graphql-ogm": "^3.16.1",
    "@neo4j/graphql-plugin-auth": "^2.1.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "graphql": "16.6.0",
    "graphql-yoga": "3.6.0",
    "neo4j-driver": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.9.5"
  }
}